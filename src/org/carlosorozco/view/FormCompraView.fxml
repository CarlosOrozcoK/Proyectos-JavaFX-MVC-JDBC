      </ImageView>
      <TextField fx:id="tfCompraId" editable="false" layoutX="181.0" layoutY="157.0" prefHeight="25.0" prefWidth="216.0" />
      <TextField fx:id="tfFecha" layoutX="166.0" layoutY="259.0" prefHeight="68.0" prefWidth="240.0" />
      <Button fx:id="btnGuardar" layoutX="14.0" layoutY="571.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="54.0" prefWidth="101.0" text="Guardar" />
      <Button fx:id="btnCancelar" layoutX="131.0" layoutY="571.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="54.0" prefWidth="101.0" text="Cancelar" />
