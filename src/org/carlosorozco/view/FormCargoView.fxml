      </ImageView>
      <TextField fx:id="tfCargoId" editable="false" layoutX="198.0" layoutY="149.0" prefHeight="48.0" prefWidth="209.0" />
      <TextField fx:id="tfNombre" layoutX="198.0" layoutY="258.0" prefHeight="99.0" prefWidth="251.0" />
      <TextArea fx:id="taDescripcion" layoutX="224.0" layoutY="409.0" prefHeight="215.0" prefWidth="251.0" />
      <Button fx:id="btnGuardar" layoutX="22.0" layoutY="659.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="48.0" prefWidth="98.0" text="Guardar" />
      <Button fx:id="btnCancelar" layoutX="149.0" layoutY="659.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="48.0" prefWidth="98.0" text="Cancelar" />
